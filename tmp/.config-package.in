	config PACKAGE_pdns-tools
		tristate "pdns-tools........................... Tools for DNS debugging by PowerDNS"
		default y if DEFAULT_pdns-tools
		default m if ALL
		select PACKAGE_libcurl
		select PACKAGE_libmbedtls
		select PACKAGE_libc
		select PACKAGE_protobuf
