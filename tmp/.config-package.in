menu "Filesystem"
	config PACKAGE_davfs2
		tristate "davfs2................. Mount a WebDAV resource as a regular file system."
		default y if DEFAULT_davfs2
		default m if ALL
		select PACKAGE_libfuse
		select PACKAGE_kmod-fuse
		select PACKAGE_libc
